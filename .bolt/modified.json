[["/home/project/src/components/Header.jsx","import React, { useState } from 'react';\nimport { FiShoppingCart, FiHome, FiBox, FiInfo, FiPhone, FiMessageSquare, FiUsers, FiBell } from 'react-icons/fi';\nimport Cart from './Cart';\n\nfunction Header() {\n  const [isCartOpen, setIsCartOpen] = useState(false);\n\n  return (\n    <header className=\"bg-white shadow-sm sticky top-0 z-50\">\n      <div className=\"container mx-auto px-4 py-4\">\n        <div className=\"flex justify-between items-center\">\n          <div className=\"flex items-center\">\n            <h1 className=\"text-2xl font-bold text-blue-600\">SkyVenda MZ</h1>\n          </div>\n          <nav className=\"hidden md:flex space-x-6\">\n            <a href=\"#\" className=\"text-gray-600 hover:text-blue-600 flex items-center\"><FiHome className=\"mr-1\" /> In√≠cio</a>\n            <a href=\"#\" className=\"text-gray-600 hover:text-blue-600 flex items-center\"><FiBox className=\"mr-1\" /> Produtos</a>\n            <a href=\"#\" className=\"text-gray-600 hover:text-blue-600 flex items-center\"><FiInfo className=\"mr-1\" /> Sobre</a>\n            <a href=\"#\" className=\"text-gray-600 hover:text-blue-600 flex items-center\"><FiPhone className=\"mr-1\" /> Contato</a>\n          </nav>\n          <div className=\"flex items-center space-x-4\">\n            <button className=\"text-gray-600 hover:text-blue-600 relative\">\n              <FiBell size={24} />\n              <span className=\"absolute top-0 right-0 bg-red-500 text-white rounded-full w-4 h-4 flex items-center justify-center text-xs\">\n                3\n              </span>\n            </button>\n            <button className=\"text-gray-600 hover:text-blue-600\">\n              <FiMessageSquare size={24} />\n            </button>\n            <button className=\"text-gray-600 hover:text-blue-600\">\n              <FiUsers size={24} />\n            </button>\n            <button\n              onClick={() => setIsCartOpen(true)}\n              className=\"text-gray-600 hover:text-blue-600 relative\"\n            >\n              <FiShoppingCart size={24} />\n              <span className=\"absolute top-0 right-0 bg-red-500 text-white rounded-full w-4 h-4 flex items-center justify-center text-xs\">\n                3\n              </span>\n            </button>\n          </div>\n        </div>\n      </div>\n      <Cart isOpen={isCartOpen} onClose={() => setIsCartOpen(false)} />\n    </header>\n  );\n}\n\nexport default Header;"]]